<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
<h1>Liste des Admins</h1>
<table border="1">
    <tr>
        <th>nom</th>
        <th>prenom</th>
        <th>email</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    {% for admin in admins %}
        <tr>
            <td>{{ admin.fname }}</td>
            <td>{{ admin.lname }}</td>
            <td>{{ admin.email }}</td>
            <td><a href="{{ path('deleteAdmin',{'id':admin.id}) }}">Delete</a></td>
            <td><a href="{{ path('updateAdmin',{'id':admin.id}) }}">Update</a></td>
        </tr>
    {% endfor %}
</table>
</body>
</html>