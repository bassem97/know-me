{% extends 'base.html.twig' %}

{% block title %}
	KnowMe
{% endblock %}


{% block body %}
	<section id="intro">
		<div class="intro-container" data-aos="zoom-in" data-aos-delay="100">
			<div class="form">
				<h1 class="mb-4 pb-0">Connectez-vous sur<br><span>KnowMe</span>
				</h1>
				<form action="/" method="post" role="form" class="php-email-form">
					<div class="form-row"></div>
					<div class="form-group">
						<input type="text" class="form-control" name="subject" id="subject" placeholder="Email" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject"/>
						<div class="validate"></div>
					</div>

					<div class="form-group">
						<input type="password" class="form-control" name="subject" id="subject" placeholder="Password" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject"/>
						<div class="validate"></div>
					</div>

					<div>
						<button class="about-btn scrollto" type="submit">Connexion
						</button>
					</div>
					<div>

						<a href="/register" class="no_account">Vous nâ€™avez pas de compte  ? Inscrivez-vous</a>
					</div>
				</form>
			</div>
		</section>
	{% endblock %}
